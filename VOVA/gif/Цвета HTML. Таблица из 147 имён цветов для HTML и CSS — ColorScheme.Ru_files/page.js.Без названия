$(function(){if($(document).height()>1500){var a=$(".scroll_up"),b=$(".scroll_down");300<$(window).scrollTop()&&a.show(),$(window).scrollTop()+1200<$(document).height()&&b.show(),$(window).scroll(function(){300<$(this).scrollTop()?a.fadeIn(200):a.fadeOut(200),$(this).scrollTop()+1200>$(document).height()?b.fadeOut(200):b.fadeIn(200)}),a.click(function(){return $("html, body").animate({scrollTop:0},300),!1}),b.click(function(){return $("html, body").animate({scrollTop:$(document).height()},300),!1})}});